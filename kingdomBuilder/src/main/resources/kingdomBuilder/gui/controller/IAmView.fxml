<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.2"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="kingdomBuilder.gui.controller.IAmViewController">

    <HBox alignment="CENTER">
        <Label text="%selectLanguage">
            <padding>
                <Insets right="5.0" />
            </padding>
        </Label>
        <ComboBox fx:id="comboBox_language" onAction="#onCheckBoxSelectionPressed" prefWidth="150.0" visibleRowCount="2" />
    </HBox>

    <Label text="%register"> </Label>
    <TextField fx:id="iAmViewTextField" maxWidth="200.0" promptText="%enterHere" />
    <Button onAction="#onButtonMainMenuShow" text="Ok" />
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

</VBox>
