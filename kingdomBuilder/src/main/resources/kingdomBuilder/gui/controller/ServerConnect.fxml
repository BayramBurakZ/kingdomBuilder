<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.2"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="kingdomBuilder.gui.controller.ServerConnectViewController">


    <TextField fx:id="textfield_address" prefHeight="25.0" prefWidth="393.0" promptText="%address"
               text="localhost"/>
    <TextField fx:id="textfield_port" prefHeight="25.0" prefWidth="134.0" promptText="%port"
               text="6666"/>

    <HBox alignment="CENTER" spacing="5.0">
        <Button maxWidth="75.0" minWidth="75.0" mnemonicParsing="false"
                onAction="#onConnectButtonPressed" text="%connect"/>
        <Button alignment="CENTER" onAction="#onBackToMenuButtonPressed" text="%backToMenu"/>
    </HBox>
</VBox>
